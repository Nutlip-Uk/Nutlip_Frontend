{
  "name": "@uploadthing/react",
  "version": "6.8.0",
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./styles.css": "./dist/index.css",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./hooks": {
      "import": {
        "types": "./hooks/index.d.ts",
        "default": "./hooks/index.js"
      },
      "require": {
        "types": "./hooks/index.d.cts",
        "default": "./hooks/index.cjs"
      }
    },
    "./native": {
      "import": {
        "types": "./native/index.d.ts",
        "default": "./native/index.js"
      },
      "require": {
        "types": "./native/index.d.cts",
        "default": "./native/index.cjs"
      }
    },
    "./next-ssr-plugin": {
      "import": {
        "types": "./next-ssr-plugin/index.d.ts",
        "default": "./next-ssr-plugin/index.js"
      },
      "require": {
        "types": "./next-ssr-plugin/index.d.cts",
        "default": "./next-ssr-plugin/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "hooks",
    "native",
    "next-ssr-plugin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@uploadthing/dropzone": "0.4.1",
    "@uploadthing/shared": "6.7.9",
    "file-selector": "^0.6.0",
    "tailwind-merge": "^2.2.1"
  },
  "peerDependencies": {
    "next": "*",
    "react": "^17.0.2 || ^18.0.0",
    "uploadthing": "6.13.3"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  }
}